{{!< default}}

{{#post}}
<article class="post">
    {{#if feature_image}}
    <img class="post-feature-image" src={{feature_image}} />
    {{/if}}
    <section class="post-section-header">
        <h1 class="post-title">{{title}}</h1>
    </section>
    <section class="post-content">
        {{content}}
    </section>
</article>
{{/post}}